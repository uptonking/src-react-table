{
  "name": "react-table",
  "version": "7.0.5",
  "description": "Hooks for building lightweight, fast and extendable datagrids for React",
  "license": "MIT",
  "homepage": "https://react-table.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/tannerlinsley/react-table"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "keywords": [
    "react",
    "table",
    "react-table",
    "datagrid"
  ],
  "main": "index.js",
  "scripts": {
    "demo": "webpack serve --hot --config ./config/webpack.config.demo.js",
    "build": "webpack --config ./config/webpack.config.demo.js",
    "test": "is-ci \"test:ci\" \"test:dev\"",
    "test:dev": "jest --watch",
    "test:ci": "yarn test:jest",
    "test:jest": "jest --coverage",
    "test:coverage": "yarn test:jest; open coverage/lcov-report/index.html",
    "build1": "cross-env NODE_ENV=production rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn build",
    "release": "yarn publish",
    "releaseNext": "yarn publish --tag next",
    "format": "prettier {src,src/**,examples/*/src,examples/*/src/**}/*.{md,js,jsx,tsx} --write",
    "docz:build": "docz build",
    "docz:dev": "docz dev",
    "docs": "docz build && docz serve",
    "docz:serve": "docz build && docz serve",
    "prettier": "prettier --config ./.prettierrc.js --loglevel log --write '**/*.{ts,tsx,js,jsx,json,css}'"
  },
  "files": [
    "CHANGELOG.md",
    "src/**/*.js",
    "dist",
    "README.md",
    "scripts/"
  ],
  "browserslist": "> 0.25%, not dead",
  "peerDependencies": {
    "react": "^16.8.3"
  },
